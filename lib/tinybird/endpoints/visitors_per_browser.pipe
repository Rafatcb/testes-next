VERSION 1

NODE visitors_per_browser
SQL >
    %
    SELECT
      browser,
      COUNT(DISTINCT session_id) AS unique_visitors,
      COUNT(*) AS views
    FROM
      page_views__v1
    WHERE
      timestamp_utc >= now() - INTERVAL {{Int32(days)}} DAY
    GROUP BY
      browser
    ORDER BY
      unique_visitors DESC
    LIMIT
      {{Int32(limit, 5)}}
